# Model Builder Started
wipe ;
model basic -ndm 2 -ndf 3 ; 
node 1 0 0 ;
node 2 0 1 ;
fix 1 1 1 1 ;
mass 2 1 0 0 ;
geomTransf Linear 1 ;
set A 1000 ;
set E 1000 ;
set IZ 1000 ; 
element elasticBeamColumn 1 1 2 $A $E $IZ 1 ;
put "Model Builder Completed"
# Model Builder Completed

# Analysis Configuration Started
set Px 1.;Â 
set Py 0.; 
set Mz 0.; 
pattern Plain 1 Linear { 
load 2 $Px $Py $Mz ; 
}

wipeAnalysis; 
constraints Plain; 
numberer Plain; 
system BandGeneral; 
test NormDispIncr 1.0e-8 6 ; 
algorithm Newton;
integrator DisplacementControl 2 1 0.1;
analysis Static
analyze 100;
# Analysis Configuration Completed
